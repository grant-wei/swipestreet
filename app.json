{
  "expo": {
    "name": "SwipeStreet",
    "slug": "swipestreet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#EDEAE5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.swipestreet.app",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "SwipeStreet does not use the camera",
        "NSPhotoLibraryUsageDescription": "SwipeStreet does not access photos"
      }
    },
    "android": {
      "package": "com.swipestreet.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#EDEAE5"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@sentry/react-native/expo",
      "@sentry/react-native"
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "@sentry/react-native/expo",
          "config": {
            "organization": "your-sentry-org",
            "project": "swipestreet"
          }
        }
      ]
    },
    "extra": {
      "eas": {
        "projectId": "8315b7f2-946a-4e45-8d59-fcd1f998a28d"
      }
    },
    "owner": "grantwei"
  }
}
